extends layout

block content
  h1 Pagina de musicas
  table 
    tr 
      th Identificador 
      th Titulo 
      th Provincia
    each m in musicas 
      tr 
        td=m.id
        td
          a(href="http://localhost:3022/musicas/#{m.id}")=m.tit
        td
          a(href="http://localhost:3022/musicas/prov/#{m.prov}")=m.prov