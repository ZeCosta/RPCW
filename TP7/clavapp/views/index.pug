extends layout

block content
  header.w3-container.w3-teal
    h1= title
  if list.length > 0
    .w3-container 
      table.w3-table-all
        tr
          th Código
          th Título
        each e in list
          tr 
            td
              a(href="/"+e.codigo)=e.codigo
            td
              a(href="/"+e.codigo)=e.titulo
  else
    .w3-container
      p Não existem entradas...
  footer.w3-container.w3-teal
    address Gerado por clavapp